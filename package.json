    "start": "yarn run build && npm link && yo typescript-package",
    "clean": "rimraf -rf ./generators"
  },
  "dependencies": {
    "yeoman-generator": "^5.6.1"
  },
  "devDependencies": {
    "@types/yeoman-generator": "^5.2.10",
    "rimraf": "^3.0.2",
