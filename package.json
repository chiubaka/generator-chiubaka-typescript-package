    "typescript",
    "npm",
    "package",
    "chiubaka-technologies"
  ],
  "author": "Daniel Chiu <daniel@chiubaka.com>",
  "license": "",
  "bugs": {
    "url": "https://github.com/chiubaka/generator-typescript-package/issues"
  },
  "homepage": "https://github.com/chiubaka/generator-typescript-package#readme",
  "scripts": {
